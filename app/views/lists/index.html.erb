<div class="card-deck d-flex">
  <% @lists.each do |l| %>
  <div class="card" style="width: 18rem;">
    <img class="card-img-top" src=<%l.image_url%> alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"><%= l.name %></h5>
      <%= link_to "View", list_path(l) %>
    </div>
  </div>
  <% end %>
  </div>
